{
    "compilerOptions": {
        "baseUrl": ".",
        "forceConsistentCasingInFileNames": true,
        "module": "amd",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "paths": {
            "axios": [
                "../../node_modules/axios"
            ],
            "services/*" : [
                "services/*"
            ]
        },
        "skipLibCheck": true,
        "sourceMap": true,
        "target": "es6",
        "typeRoots": [
            "../../node_modules/@types",
            "../../types"
        ]
    }
}
